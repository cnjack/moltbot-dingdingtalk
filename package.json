{
  "name": "moltbot-dingtalk-stream",
  "version": "1.1.0",
  "description": "DingTalk custom channel plugin for Moltbot/OpenClaw",
  "type": "module",
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "moltbot-dingtalk-stream",
      "label": "DingTalk",
      "selectionLabel": "DingTalk Bot (Stream)",
      "docsPath": "/channels/dingtalk",
      "docsLabel": "dingtalk",
      "blurb": "DingTalk bot channel plugin (Stream mode)",
      "aliases": [
        "dt",
        "ding",
        "dingtalk"
      ],
      "order": 100
    },
    "install": {
      "npmSpec": "moltbot-dingtalk-stream",
      "localPath": "extensions/moltbot-dingtalk-stream",
      "defaultChoice": "npm"
    }
  },
  "scripts": {
    "typecheck": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch"
  },
  "keywords": [
    "moltbot",
    "openclaw",
    "plugin",
    "dingtalk",
    "channel",
    "chatbot"
  ],
  "author": "Jack",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dingtalk-stream": "^2.1.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}